#include <gtest/gtest.h>
#include <iostream>

using namespace std;

bool myFun() {
     return true;
  }

int main(int argc, char *argv[]) {
  return 0;
}

TEST(MyFunTest, DefaultTest) {
  EXPECT_EQ(myFun(), true);
}
